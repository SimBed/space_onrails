<%= form_for(@attendance) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  
  <%= f.hidden_field :members_package, :value => object.id%>

  <%#= f.label :date %>
  <%= f.date_field :date, class: 'form-control' %>
  
   <%= f.submit "Add Class", class: "btn btn-primary" %>
<% end %>